	<build>
		<finalName>odata2sparql.v4</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.1</version>
					<configuration>
						<source>8</source>
						<target>8</target>
						<showDeprecation>true</showDeprecation>
					</configuration>
				</plugin>
				<plugin>
          			<artifactId>maven-war-plugin</artifactId>
          			<version>3.2.3</version>
        			</plugin> 
			</plugins>
		</pluginManagement>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
	</build>
	<organization>
		<name>inova8 llc</name>
		<url>http://www.inova8.com</url>
	</organization>
	<scm>
		<developerConnection>Peter Lawrence</developerConnection>
		<url>https://github.com/peterjohnlawrence/com.inova8.odata2sparql.v4</url>
	</scm>
	<properties>
		<olingo.version>4.6.0</olingo.version>
