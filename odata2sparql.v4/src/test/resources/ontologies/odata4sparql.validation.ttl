odata4sparql:Operation
  spin:constraint [
      rdf:type sp:Ask ;
      sp:text """# query must be declared
ASK WHERE {
    ?this spin:body ?query .
   FILTER(!BOUND(?query)) 
}""" ;
      sp:where (
          [
            sp:object [
                sp:varName "query" ;
              ] ;
            sp:predicate spin:body ;
            sp:subject spin:_this ;
          ]
          [
            rdf:type sp:Filter ;
            sp:expression [
                rdf:type sp:not ;
                sp:arg1 [
                    rdf:type sp:bound ;
                    sp:arg1 [
                        sp:varName "query" ;
                      ] ;
                  ] ;
              ] ;
          ]
        ) ;
      rdfs:comment "query must be declared" ;
    ] ;
